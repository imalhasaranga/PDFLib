{
    "name": "imal-h/pdf-box",
    "description": "Current Release of PDFlib provide you methods to convert PDF to Images as well as Images to PDF.",
    "keywords": [
        "Imal hasaranga",
        "PDFlib for Php",
        "pdf to images",
        "images to pdf",
        "convert",
        "php wrapper",
        "GhostScript"
    ],
    "homepage": "https://github.com/imalhasaranga/PDFLib.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Imal hasaranga Perera",
            "email": "imal.hasaranga@gmail.com",
            "homepage": "http://www.treinetic.com",
            "role": "CEO"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/process": "^6.4 || ^7.0",
        "tecnickcom/tcpdf": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ImalH\\PDFLib\\": "src"
        }
    },
    "autoload-dev": {},
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "analyze": "vendor/bin/phpstan analyse src --level 5"
    }
}
