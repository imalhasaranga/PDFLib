{
    "name": "imal-h/pdf-box",
    "description": "The most advanced, driver-based PDF manipulation library for PHP v3.0. Supports Ghostscript, Chrome Headless (HTML to PDF), OpenSSL (Signing), and PDFtk (Forms).",
    "keywords": [
        "pdf",
        "php",
        "manipulation",
        "html-to-pdf",
        "digital-signature",
        "form-filling",
        "ghostscript",
        "pdftk",
        "chrome-headless",
        "driver-based"
    ],
    "homepage": "https://github.com/imalhasaranga/PDFLib.git",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/imalhasaranga/PDFLib/issues",
        "source": "https://github.com/imalhasaranga/PDFLib",
        "docs": "https://github.com/imalhasaranga/PDFLib/tree/master/docs"
    },
    "authors": [
        {
            "name": "Imal hasaranga Perera",
            "email": "imal.hasaranga@gmail.com",
            "homepage": "http://www.treinetic.com",
            "role": "CEO"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-fileinfo": "*",
        "symfony/process": "^6.4 || ^7.0",
        "tecnickcom/tcpdf": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "ImalH\\PDFLib\\": "src"
        }
    },
    "autoload-dev": {},
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "analyze": "vendor/bin/phpstan analyse src --level 5"
    }
}